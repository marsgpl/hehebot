{
  "name": "hehe-bot",
  "version": "0.1.1",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "watch": "tsc-watch --onSuccess 'node --experimental-json-modules build/index.js'",
    "build": "tsc --project . --incremental false --outDir build.prod",
    "uglify": "uglifyjs-folder build.prod --each --extension .js --output build.uglified"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "koa": "^2.13.1",
    "koa-ejs": "^4.3.0"
  },
  "devDependencies": {
    "@types/koa": "^2.13.1",
    "@types/koa-ejs": "^4.2.2",
    "@types/node": "^15.6.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.2.4",
    "uglifyjs-folder": "^2.0.0"
  }
}
